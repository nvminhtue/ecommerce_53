<div class="row fix admin-row">
<!-- left nav -->
<div class="col-sm-2 fix admin-nav-left">
  <div class="navbar-default sidebar" role="navigation">
  <div class="sidebar-nav navbar-collapse my-sidebar">
    <ul class="nav in" id="side-menu">
      <li class="sidebar-search">
        <div class="input-group custom-search-form">
          <input type="text" class="form-control" placeholder="Search...">
            <span class="input-group-btn">
              <button class="btn btn-default" type="button">
                <i class="fa fa-search"></i>
              </button>
            </span>
          </div>
        </li>
        <!-- Admin menu -->

        <li>
          <%= link_to admin_root_path, id: "dashboard" do %>
            <i class="fa fa-dashboard fa-fw"></i> <%= t ".dash" %>
          <% end %>
        </li>


        <!-- Block menu user -->
        <li class="admin-menu-left">
          <div class="admin-menu-click" id="admin-menu-1" onclick="hs(this.getAttribute('id'))">
            <i class="fa fa-users"></i> <%= t ".mana_user" %>
            <i class="fa fa-angle-left btright" aria-hidden="true"></i>
          </div>

        </li>
        <div class="admin-hs hide" id="for-menu-1">
          <%= link_to admin_users_path, class: "go", id: "list_user" do %>
            <i class="fa fa-list-ul" aria-hidden="true"></i> List User
          <% end %>
          <%= link_to admin_suggestions_path, class: "go", id: "suggestion" do %>
              <i class="fa fa-paper-plane" aria-hidden="true"></i> <%= t ".mana_suggest" %>
          <% end %>
        </div>
        <!-- /Block menu user -->

        <!-- Block menu product -->
        <li class="admin-menu-left">
          <div class="admin-menu-click" id="admin-menu-2" onclick="hs(this.getAttribute('id'))">
            <i class="fa fa-tags" aria-hidden="true"></i> <%= t ".mana_prod" %>
            <i class="fa fa-angle-left btright" aria-hidden="true"></i>
          </div>

        </li>

        <div class="admin-hs hide" id="for-menu-2">
          <%= link_to admin_products_path, class: "go", id: "products_list" do %>
            <i class="fa fa-list" aria-hidden="true"></i> List Product
          <% end %>
          <%= link_to import_admin_products_path, class: "go", id: "products_import" do %>
            <i class="fa fa-cloud" aria-hidden="true"></i> <%= t ".mana_import" %>
          <% end %>
          <%= link_to admin_categories_path, class: "go", id: "products_cate" do %>
              <i class="fa fa-industry" aria-hidden="true"></i> <%= t ".mana_cate" %>
          <% end %>
          <%= link_to admin_orders_path, class: "go", id: "products_order" do %>
            <i class="fa fa-money" aria-hidden="true"></i> <%= t ".mana_orders" %>
          <% end %>
        </div>
        <!-- /Block menu product -->

        <!-- /Admin menu -->
      </ul>
    </div>
  </div>
</div>
<!-- /left nav -->

<!-- right nav -->
<div class="col-sm-10 admin-right">
